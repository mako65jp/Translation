{
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "prettier": true,
        "class-name": true,
        "curly": true,
        "no-duplicate-variable": true,
        "no-string-throw": true,
        "no-unused-expression": true,
        "triple-equals": true
    },
    "defaultSeverity": "warning",
    "extends": ["tslint-config-prettier"]
}
